public class Point {
  
  private float x, y;
 
  public Point(float newX, float newY) {
    x = newX;
    y = newY; 
  }
  
  
  public float getX() {
    return x; 
  }
  
  
  public void setX(float newX) {
    x = newX;
  }
  
  
  public float getY() {
    return y;
  }
  
  
  public void setY(float newY) {
    y = newY;
  }
  
}
